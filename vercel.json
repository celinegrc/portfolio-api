{
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "server.js"
        }
    ],
        "headers": [
          {
            "source": "/post",
            "headers": [
              { "key": "Access-Control-Allow-Origin", "value": "https://portfolio-frontend-celinegrc.vercel.app" }
            ]
          }
        ]   
}